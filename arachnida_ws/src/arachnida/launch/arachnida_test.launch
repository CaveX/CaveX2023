<launch>
    
	<!-- Launch velodyneReaderNode as the nodelet manager (required to run nodelets in velodyneReaderNode's process) -->
    <!-- type="velodyneReaderNode" -->
    <node 
    pkg="arachnida" 
    type="velodyneReaderNode" 
    name="velodyneReaderNode"
    args="manager"
    output="screen"
    />
	
	<!-- Launch the nodelet manager (required to run nodelets) -->
	
	<!-- START: Launch the nodelets -->
	<node 
	pkg="nodelet"
	type="nodelet"
	name="Hello_World"
	args="load arachnida/Hello_World velodyneReaderNode"
	output="screen"
	/>
	
	<node 
	pkg="nodelet"
	type="nodelet"
	name="FLOAMNoelet"
	args="load arachnida/FLOAMNodelet velodyneReaderNode"
	output="screen"
	/>
	<node 
	pkg="nodelet"
	type="nodelet"
	name="ObjectDetectionNodelet"
	args="load arachnida/ObjectDetectionNodelet velodyneReaderNode"
	output="screen"
	/>
	<node 
	pkg="nodelet"
	type="nodelet"
	name="PathPlanningNodelet"
	args="load arachnida/PathPlanningNodelet velodyneReaderNode"
	output="screen"
	/>
	<node 
	pkg="nodelet"
	type="nodelet"
	name="RobotStateNodelet"
	args="load arachnida/RobotStateNodelet velodyneReaderNode"
	output="screen"
	/>
	
	<!-- END: Launch the nodelets -->
</launch>
